{
  "name": "discrete-time",
  "version": "0.2.1",
  "description": "Easily step through discrete time intervals, calling functions as you go!",
  "main": "index.js",
  "scripts": {
    "preversion": "npm run lint && npm test && npm run coverage",
    "postversion": "npm run changelog && git commit -am 'update changelog'",
    "lint": "grunt jshint",
    "test": "grunt mocha_istanbul",
    "coverage": "grunt istanbul_check_coverage",
    "document": "grunt jsdoc",
    "changelog": "grunt githubChanges"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willdavis/discrete-time.git"
  },
  "keywords": [
    "discrete",
    "time",
    "simulation",
    "moment"
  ],
  "author": "William Davis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/willdavis/discrete-time/issues"
  },
  "homepage": "https://github.com/willdavis/discrete-time#readme",
  "dependencies": {
    "moment": "^2.15.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "grunt": "^1.0.1",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-github-changes": "^0.1.0",
    "grunt-jsdoc": "^2.1.0",
    "grunt-mocha-istanbul": "^5.0.2",
    "ink-docstrap": "^1.3.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.1",
    "jshint": "^2.9.3",
    "mocha": "^3.0.2"
  }
}
